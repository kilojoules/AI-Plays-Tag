{ "type": "InputEventKey", "keycode": 68, "physical_keycode": 68, "echo": false, "pressed": false },
{ "type": "InputEventKey", "keycode": 4194321, "physical_keycode": 4194321, "echo": false, "pressed": false }
]
}
jump={
"deadzone": 0.5,
"events": [ { "type": "InputEventKey", "keycode": 32, "physical_keycode": 32, "echo": false, "pressed": false } ]
}
switch_agent={
"deadzone": 0.5,
